{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/menu": [
        "src/components/menu/*"
      ],
      "@/hooks": [
        "src/hooks/*"
      ],
      "@/styles": [
        "src/styles/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "extends": "astro/tsconfigs/strict"
}